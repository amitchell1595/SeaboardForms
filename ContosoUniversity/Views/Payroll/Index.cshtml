@model IEnumerable<ContosoUniversity.Models.Payroll>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PayrollName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PayrollName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PayrollID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PayrollID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PayrollID })
        </td>
    </tr>
}

</table>
